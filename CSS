        max-width: calc(25% - var(--_space, 1.8rem) * 1 * var(--_margin-scale, 1) * 3/4)
    }

    .smb-spider-slider--gutter-m[data-md-slide-to-show="5"] .spider__slide {
        max-width:calc(20% - var(--_space, 1.8rem) * 1 * var(--_margin-scale, 1) * 4/5)
    }

